{
  "name": "learning-app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "deploy": "expo export --platform web && npx serve dist",
    "deploy:netlify": "npm run build:web && netlify deploy --prod --dir=dist",
    "deploy:vercel": "npm run build:web && vercel --prod",
    "deploy:render": "npm run build:web && render deploy",
    "deploy:railway": "npm run build:web && railway up",
    "deploy:firebase": "npm run build:web && firebase deploy",
    "deploy:surge": "npm run build:web && cd dist && surge .",
    "deploy:heroku": "npm run build:web && git add . && git commit -m 'Deploy' && git push heroku main",
    "deploy:aws": "npm run build:web && aws s3 sync dist/ s3://your-bucket-name --delete",
    "deploy:azure": "npm run build:web && swa deploy dist",
    "deploy:cloudflare": "npm run build:web && wrangler pages publish dist",
    "deploy:digitalocean": "npm run build:web && doctl apps create --spec .do/app.yaml",
    "lint": "expo lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "clean": "rm -rf dist .expo node_modules/.cache"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "expo": "~51.0.0",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-paper": "^5.12.3",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@types/react": "~18.2.45",
    "typescript": "~5.3.3"
  },
  "private": true
}
